{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"border": {},
		"blocks": {
			"core/image": {
				"lightbox": {
					"allowEditing": false
				}
			}
		},
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#111",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#eee",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#888",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#333",
					"name": "Light Black",
					"slug": "light-black"
				},
				{
					"color": "#666",
					"name": "Dark Grey",
					"slug": "dark-grey"
				},
				{
					"color": "#999",
					"name": "Light Grey",
					"slug": "light-grey"
				},
				{
					"color": "#ccc",
					"name": "Dark White",
					"slug": "dark-white"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				}
			]
		},
		"dimensions": {
			"minHeight": true
		},
		"layout": {
			"contentSize": "44rem",
			"wideSize": "75rem"
		},
		"position": {
			"sticky": true
		},
		"shadow": {
			"defaultPresets": true
		},
		"spacing": {
			"blockGap": null,
			"customSpacingSize": false,
			"margin": true,
			"padding": true,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(0.748125rem, 0.42rem + 1.313vw, 0.9975rem)",
					"slug": "20",
					"name": "X-Small"
				},
				{
					"size": "clamp(1.49625rem, 0.849rem + 2.605vw, 1.995rem)",
					"slug": "30",
					"name": "Small"
				},
				{
					"size": "clamp(2.9925rem, 1.68rem + 5.25vw, 3.99rem)",
					"slug": "40",
					"name": "Medium"
				},
				{
					"size": "clamp(4.48875rem, 2.52rem + 7.875vw, 5.985rem)",
					"slug": "50",
					"name": "Large"
				}
			]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fontSizes": [
				{
					"fluid": {
						"min": "0.75em",
						"max": "1em"
					},
					"name": "X Small",
					"size": "1em",
					"slug": "x-small"
				},
				{
					"fluid": {
						"min": "0.9975em",
						"max": "1.333em"
					},
					"name": "Small",
					"size": "1.333em",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1.33275em",
						"max": "1.777em"
					},
					"name": "Medium",
					"size": "1.777em",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.777em",
						"max": "2.369em"
					},
					"name": "Large",
					"size": "2.369em",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2.36775em",
						"max": "3.157em"
					},
					"name": "X Large",
					"size": "3.157em",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "3.15675em",
						"max": "4.209em"
					},
					"name": "XX Large",
					"size": "4.209em",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "4.2075em",
						"max": "5.61em"
					},
					"name": "XXX Large",
					"size": "5.61em",
					"slug": "xxx-large"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": false,
			"textColumns": false,
			"textDecoration": true,
			"textTransform": true,
			"writingMode": false
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/list": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var:preset|spacing|30"
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"lineHeight": "1lh"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var:preset|spacing|30"
					}
				}
			},
			"core/pullquote": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"lineHeight": "var:preset|spacing|40"
				},
				"spacing": {
					"margin": {
						"top": "var:preset|spacing|40",
						"bottom": "var:preset|spacing|40"
					},
					"padding": {
						"top": "calc( var(--wp--preset--spacing--40) - 4px )",
						"bottom": "calc( var(--wp--preset--spacing--40 ) - 4px )"
					}
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"lineHeight": "var:preset|spacing|30"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var:preset|spacing|30"
					}
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|xx-large",
					"lineHeight": "var:preset|spacing|50"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|x-large",
					"lineHeight": "var:preset|spacing|40"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"lineHeight": "var:preset|spacing|30"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"lineHeight": "var:preset|spacing|30"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"lineHeight": "var:preset|spacing|30"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"lineHeight": "var:preset|spacing|30",
					"fontStyle": "italic",
					"fontWeight": "normal"
				}
			},
			"heading": {
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var:preset|spacing|30"
					}
				}
			}
		},
		"spacing": {
			"margin": {
				"top": "0",
				"bottom": "var:preset|spacing|30"
			},
			"padding": {
				"left": "var:preset|spacing|30",
				"right": "var:preset|spacing|30"
			}
		},
		"typography": {
			"fontSize": "var:preset|font-size|small",
			"lineHeight": "clamp(1.49625rem, 0.849rem + 2.605vw, 1.995rem)"
		}
	},
	"templateParts": [
		{
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "uncategorized",
			"name": "main",
			"title": "Main Content"
		},
		{
			"area": "uncategorized",
			"name": "sidebar",
			"title": "Sidebar"
		}
	]
}